{
    "env": {
      "browser": true,
      "es2021": true,
      "amd": true,
      "jest": true,
      "node": true,
      "cypress/globals": true
    },
    "extends": "eslint:recommended",
    "overrides": [
      {
        "files": ["**/*.cy.js"],
        "env": { "cypress/globals": true },
        "plugins": ["cypress"],
        "extends": ["plugin:cypress/recommended",
        "eslint:recommended"],
        
        "rules": {
          "cypress/no-unnecessary-waiting": "off",
          "no-unused-vars": "off"
        }
      }
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules":{
      
      "no-unused-vars": "warn"
    }
  }